		clocks = <&clock 593>, <&clock 567>;
		status = "ok";
	};

	decon_fb: decon_fb {
		compatible = "samsung,exynos5-decon";
		reg = <0x13800000 0x40000>;
		samsung,vidcon0 = <0x00>;
		samsung,vidcon1 = <0x80>;
		samsung,default_win = <0>;

		samsung,left_margin = <20>;
		samsung,right_margin = <20>;
		samsung,upper_margin = <3>;
		samsung,lower_margin = <20>;
		samsung,hsync_len = <20>;
		samsung,vsync_len = <2>;
		samsung,xres = <1080>;
		samsung,yres = <1920>;
		samsung,virtual_x = <1080>;
		samsung,virtual_y = <1920>;
		samsung,width = <71>;
		samsung,height = <114>;
		samsung,max_bpp = <32>;
		samsung,default_bpp = <24>;

		samsung,i80_irq_no = <233>;
		samsung,irq_no = <234>;

		clock-names = "lcd", "sclk_eclk", "sclk_vclk", "decon_dsd";
		clocks = <&clock 321>, <&clock 100>, <&clock 101>, <&clock 102>;

		iommu = <&sysmmu_decon0x>, <&sysmmu_decon1x>;
	};

	mipi_dsi: mipi_dsi {
		compatible = "samsung,exynos5-dsim";
		reg = <0x13900000 0x40000>;
		samsung,irq_no = <237>;

		samsung,left_margin = <20>;
		samsung,right_margin = <20>;
		samsung,upper_margin = <3>;
		samsung,lower_margin = <20>;
		samsung,hsync_len = <20>;
		samsung,vsync_len = <2>;
		samsung,xres = <1080>;
		samsung,yres = <1920>;
	};

	watchdog@10020000 {
		compatible = "samsung,s3c2410-wdt";
		reg = <0x10020000 0x100>;
		interrupts = <0 101 0>;
		clocks = <&clock 870>;
		clock-names = "watchdog";
		timeout-sec = <30>;
	};

	firmware@0207F000 {
		compatible = "samsung,secure-firmware";
		reg = <0x0207F000 0x1000>;
	};

	usb@15400000 {
		compatible = "samsung,exynos5-dwusb3";
		clocks = <&clock 50>, <&clock 140>, <&clock 561>;
		clock-names = "sclk_usbdrd30", "usbdrd30", "aclk_usbdrd30";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		status = "disabled";

		dwc3 {
			compatible = "synopsys,dwc3";
			reg = <0x15400000 0x10000>;
			interrupts = <0 231 0>;
			usb-phy = <&dwc3_usb2_phy &dwc3_usb3_phy>;
		};
	};

	dwc3_usb2_phy: usbphy {
		compatible = "samsung,exynos5-usb2phy-dummy";
	};

	dwc3_usb3_phy: usbphy@15500000 {
		compatible = "samsung,exynos5-usb3phy";
		reg = <0x15500000 0x100>;
		clocks = <&clock 1>, <&clock 140>, <&clock 410>, <&clock 411>;
		clock-names = "ext_xtal", "usbdrd30", "usbdrd30_phyclock", "usbdrd30_pipe_pclk";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		usbphy-sys {
			reg = <0x105C0704 0xC>;
		};
	};
};
