		clock-names = "sysmmu", "master";
		clocks = <&clock 590>, <&clock 560>;
		status = "ok";
	};

	sysmmu_mmc0: sysmmu@0x15660000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x15660000 0x1000>;
		interrupts = <0 233 0>;
		clock-names = "sysmmu", "master";
		clocks = <&clock 591>, <&clock 565>;
		status = "ok";
	};

	sysmmu_mmc1: sysmmu@0x15670000 {
		compatible = "samsung,exynos5430-sysmmu";
