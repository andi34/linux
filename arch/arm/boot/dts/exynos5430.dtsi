		      <0x15210000 0x1000>;
		interrupts = <0 352 0>,
			     <0 354 0>;
		status = "ok";
	};

	sysmmu_mfc1: sysmmu@0x15300000 {
		compatible = "samsung,exynos5430-sysmmu";
