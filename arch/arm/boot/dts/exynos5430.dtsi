	sysmmu_gscl2: sysmmu@0x13CA0000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x13CA0000 0x1000>;
		interrupts = <0 292 0>;
		clock-names = "sysmmu", "master";
		clocks = <&clock 492>, <&clock 482>;
		status = "ok";
	};

	sysmmu_flite2: sysmmu@0x142B0000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x142B0000 0x1000>;
		interrupts = <0 160 0>;
		status = "ok";
	};

	sysmmu_isp0: sysmmu@0x142C0000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x142C0000 0x1000>,
		      <0x142D0000 0x1000>,
		      <0x14320000 0x1000>,
		      <0x14330000 0x1000>,
		      <0x14340000 0x1000>;
		interrupts = <0 162 0>,
			     <0 169 0>,
			     <0 346 0>,
			     <0 338 0>,
			     <0 340 0>;
		status = "ok";
	};

	sysmmu_isp1: sysmmu@0x143A0000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x143A0000 0x1000>,
		      <0x143B0000 0x1000>,
		      <0x143D0000 0x1000>;
		interrupts = <0 342 0>,
			     <0 344 0>,
			     <0 349 0>;
		status = "ok";
	};

	sysmmu_isp2: sysmmu@0x143C0000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x143C0000 0x1000>;
		interrupts = <0 336 0>;
		status = "ok";
	};

	sysmmu_hevc: sysmmu@0x14F00000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x14F00000 0x1000>,
		      <0x14F10000 0x1000>;
		interrupts = <0 449 0>,
			     <0 451 0>;
		status = "ok";
	};

	sysmmu_scaler0: sysmmu@0x15040000 {
		compatible = "samsung,exynos5430-sysmmu";
		reg = <0x15040000 0x1000>;
		interrupts = <0 404 0>;
		clock-names = "sysmmu", "master";
		clocks = <&clock 530>, <&clock 520>;
		status = "ok";
	};

	sysmmu_scaler1: sysmmu@0x15050000 {
		compatible = "samsung,exynos5430-sysmmu";
