/dts-v1/;
#include "exynos3250.dtsi"


/ {
	model = "Samsung ESPRESSO3250 board based on EXYNOS3250";
	compatible = "samsung,ESPRESSO3250", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M";
	};

	memory {
		reg = <0x40000000 0x20000000>;
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos3250-oscclk";
			clock-frequency = <24000000>;
		};
		half_mpll {
			compatible = "samsung,exynos3250-half_mpll";
			clock-frequency = <400000000>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		button@1 {
			label = "gpio-keys: KEY_POWER";
			linux,code = <116>;
			gpios = <&gpx2 7 0x1>;
			gpio-key,wakeup = <1>;
			debounce-interval = <0>;
		};

		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			linux,code = <114>;
			gpios = <&gpx1 7 0x1>;
			debounce-interval = <0>;
		};

		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			linux,code = <115>;
			gpios = <&gpx0 0 0x1>;
			debounce-interval = <0>;
		};

		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			linux,code = <172>;
			gpios = <&gpx0 3 0x1>;
			debounce-interval = <0>;
		};
	};
};
